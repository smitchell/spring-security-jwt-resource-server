# Create Groups
dn: cn=external,ou=groups,dc=example,dc=com
objectclass: top
objectclass: groupOfUniqueNames
cn: external
ou: external
uniqueMember: uid=john,ou=people,dc=example,dc=com
uniqueMember: uid=jane,ou=people,dc=example,dc=com

